{{>partials/header}}

<section class="banner" style="background-image: url('images/{{ banner.background }}')">
  {{ banner.title }}
</section>
<h2>Todos os animais disponíveis para adoção</h2>

<div class="container list">
  {{#list}}
    <div class="item">
      <img src="images/{{ image }}" class="item--image">
      <div class="item--name">{{ name }}</div>
      <div class="item--color">Cor: {{ color }}</div>    </div>
  {{/list}}
</div>
{{>partials/footer}}